<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.door.match.dao.CommonDao">

    <select id="ageRankAll" resultType="com.door.match.entity.AgeRank">
        select id,agerank  from age_rank
    </select>

    <select id="salaryRankAll" resultType="com.door.match.entity.SalaryRank">
             select id,salary from salary_rank
    </select>
    <select id="professionAll" resultType="com.door.match.entity.Profession">
            select id,name,parentid from profession
    </select>


</mapper>